{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "name": "@flowcore/data-pump",
  "description": "Flowcore Data Pump",
  "version": "0.0.1",
  "license": "MIT",
  "exports": "./src/mod.ts",
  "publish": {
    "include": [
      "src/**/*.ts",
      "README.md",
      "CHANGELOG.md"
    ]
  },
  "tasks": {},
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.41.3",
    "@sinclair/typebox": "npm:@sinclair/typebox@0.32.15"
  },
  "fmt": {
    "exclude": [
      "node_modules",
      "npm"
    ],
    "lineWidth": 120,
    "indentWidth": 2,
    "useTabs": false,
    "semiColons": false
  },
  "lint": {
    "exclude": [
      "node_modules",
      "npm"
    ]
  }
}
